<%- include('partials/header') %>


    <nav class="navbar navbar-expand-lg ">
        <a class="navbar-brand black" href="/">
            <img src="https://tse1.mm.bing.net/th?id=OIP.BrvR9-atH0KR2dbpeW0wxAHaF7&pid=Api&P=0&h=220" alt="logo"
                style="height: 4em;">
            Carental
        </a>
        <div style="margin-left: 21.3rem;">
            <h1 class="display-6">Admin</h1>
        </div>

        <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent"
            aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
        </button>

        <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <ul class="navbar-nav mb-2 mb-lg-0 ms-auto">

                <li class="nav-item ">
                    <a class="nav-link display-6 " href="/admin">Back</a>
                </li>

                <li class="nav-item">
                    <a class="nav-link  display-6" href="#footer">Contact us</a>
                </li>
            </ul>
        </div>
    </nav>


    <div class="row ">
        <div class="justify-content-center">
            <div className="page-title">
                <h3 class="page-title display-6">View all Offices</h3>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-12">
            <table class="table table-striped">
                <thead>
                    <tr>
                        <th scope="col">Plate ID</th>
                        <th scope="col">Model</th>
                        <th scope="col">Make</th>
                        <th scope="col">Color</th>
                        <th scope="col">body_type</th>
                        <th scope="col">year</th>
                        <th scope="col">price</th>
                        <th scope="col">registration_date</th>
                        <th scope="col">office_id</th>
                    </tr>
                </thead>
                <tbody>
                    <% if (cars) { %>
                        <% cars.forEach(Office=> { %>
                            <tr>
                                <td>
                                    <%= Office.plate_id %>
                                </td>
                                <td>
                                    <%= Office.model %>
                                </td>
                                <td>
                                    <%= Office.make %>
                                </td>
                                <td>
                                    <%= Office.color %>
                                </td>
                                <td>
                                    <%= Office.body_style %>
                                </td>
                                <td>
                                    <%= Office.year %>
                                </td>
                                <td>
                                    <%= Office.price %>
                                </td>
                                <td>
                                    <%= Office.registration_date %>
                                </td>
                                <td>
                                    <%= Office.office_id %>
                                </td>
                            </tr>
                            <% }); %>
                                <% } %>
                </tbody>
            </table>
        </div>
        <%- include('partials/footer') %>